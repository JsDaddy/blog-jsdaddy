header
    width: 100%
    min-height: 85px
    display: flex
    flex-direction: row
    align-items: center
    padding-top: 25px
    z-index: 50
    top: 0
    left: 0
    transition: width 0.15s ease, padding-top ease 0.15s, min-height ease 0.15s
    box-shadow: 0px 0px 10px rgba(224, 224, 224, 0.9)
    .nav-wrapper
        nav
            ul
                li
                    a
                        color: #232422;
                        font-size: 16px;
                        font-weight: 600 !important
                        //font-family: "robotoMedium", sans-serif;
